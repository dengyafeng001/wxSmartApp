/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  width: 100px;
  height: 30px;
  margin-bottom: 1px;
  position: fixed;
}

.cellbg{
  background-color: #f5f5f5;
}

.celltitle{
  flex: 1;
  height: 44px;
  line-height: 44px;
  margin-left: 15px;
}

.sepline{
  background-color: #dddddd;
  height: 1px;
}